  "query": "\n        INSERT INTO \"comments\" (\"review_id\", \"user_id\", \"parent_id\", \"content\", \"created_at\")\n        SELECT $1, $2, $3, $4, NOW()\n        WHERE EXISTS (\n            SELECT \"review_id\"\n            FROM \"reviews\"\n            WHERE \"review_id\" = $1\n        )\n        RETURNING \"comment_id\", \"review_id\", \"user_id\", \"parent_id\", \"content\", \"created_at\";\n        ",
